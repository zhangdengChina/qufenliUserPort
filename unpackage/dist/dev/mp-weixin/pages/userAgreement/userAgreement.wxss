.common-header {
  height: 100rpx;
  font-size: 40rpx;
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: cover;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  color: #fff;
  position: fixed;
  width: 100%;
  z-index: 99999;
  top: 0;
  padding-top: 40rpx;
}
.common-header > .iconfont {
  position: absolute;
  left: 30rpx;
}
.common-header .title {
  width: 100%;
  text-align: center;
}
.common-header .right {
  position: absolute;
  right: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
.common-header .right .iconfont {
  margin-left: 20rpx;
}
.common-header .right .icon-lingdang-xianxing {
  font-size: 38rpx;
}
.common-header .right .icon-fenxiang {
  font-size: 30rpx;
}
.common-header .right .icon-gengduo {
  display: none;
}
.common-header .right .xingHide {
  display: none;
}
.common-header .right .lingHide {
  display: none;
}
.common-header .right .fenxiangHide {
  display: none;
}
.common-header .right .gengduoHide {
  display: block;
}
.maskShow {
  display: none;
}
.mask {
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background: rgba(0, 0, 0, 0.8);
  z-index: 9999999;
  font-size: 30rpx;
}
.mask .mask-top {
  position: absolute;
  top: 0;
  min-height: 75%;
  width: 100%;
}
.mask .mask-content {
  height: 376rpx;
  width: 100%;
  background: #fff;
  position: absolute;
  bottom: 0;
}
.mask .mask-content .mask-content-cancel {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 30rpx;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
.mask .mask-content .mask-content-cancel text {
  font-size: 40rpx;
  color: #999;
}
.mask .mask-content .mask-content-title {
  text-align: center;
  font-weight: bold;
  color: #D8D8D8;
}
.mask .mask-content .mask-content-title text {
  color: #000;
  font-size: 30rpx;
  margin: 0 20rpx;
}
.mask .mask-content .mask-content-item {
  margin-top: 40rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  color: #666;
  font-size: 28rpx;
  text-align: center;
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.mask .mask-content .mask-content-item > view > view {
  margin-bottom: 10rpx;
}
.mask .mask-content .mask-content-item .iconfont {
  font-size: 60rpx;
}
.mask .mask-content .mask-content-item image {
  width: 80rpx;
  height: 80rpx;
}
.mask .mask-content .mask-content-item .weixin .iconfont {
  font-size: 100rpx;
  color: #1bac19;
}
.mask .mask-content .mask-content-item .pengyouquan view {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 1px solid #e0e0e0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.mask .mask-content .mask-content-item .weibo view,
.mask .mask-content .mask-content-item .qq view {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  color: #fff;
}
.mask .mask-content .mask-content-item .weibo view {
  background: #FF9206;
}
.mask .mask-content .mask-content-item .qq view {
  background: #4DCFFC;
}

.userAgreem {
  height: 100%;
  padding-top: 120rpx;
}
.userAgreem .content {
  padding: 20rpx;
}

