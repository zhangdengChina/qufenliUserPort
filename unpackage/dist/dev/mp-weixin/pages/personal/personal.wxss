}
.personal .personal-top .personal-top-left .title .dengji text {
  font-size: 50rpx;
}
.personal .personal-top .personal-top-right .iconfont {
  font-size: 50rpx;
}
.personal .personal-msg {
  position: relative;
  top: -70rpx;
  background: #fff;
  width: 95%;
  margin: auto;
  border-radius: 20rpx;
}
.personal .personal-msg > view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.personal .personal-msg .personal-msg-top {
  border-bottom: 1px solid #ccc;
}
.personal .personal-msg .personal-msg-top > view {
  width: 33%;
  padding: 30rpx 0;
  text-align: center;
}
.personal .personal-msg .personal-msg-top > view text {
  font-size: 38rpx;
  color: #FF5A32;
  font-weight: bold;
}
.personal .personal-msg .personal-msg-top > view view {
  margin-top: 25rpx;
  font-size: 30rpx;
}
.personal .personal-msg .personal-msg-top > view:not(:last-child) {
  border-right: 1px solid #ccc;
}
.personal .personal-msg .personal-msg-bottom {
  padding: 30rpx 0;
}
.personal .personal-msg .personal-msg-bottom .text:first-child {
  border-right: 1px solid #ccc;
}
.personal .personal-msg .personal-msg-bottom .text {
  width: 50%;
  text-align: center;
}
.personal .personal-msg .personal-msg-bottom .text text {
  font-weight: bold;
  color: #FF5A32;
}
.personal .personal-msg .personal-msg-bottom .text .xingbi {
  font-size: 30rpx;
  margin: 20rpx 0;
}
.personal .personal-msg .personal-msg-bottom .text .duihuan {
  width: 156rpx;
  height: 50rpx;
  border-radius: 25rpx;
  background: #f2e1db;
