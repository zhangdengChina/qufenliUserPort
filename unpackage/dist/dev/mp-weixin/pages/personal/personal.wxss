.personal {
  min-height: 100%;
  background: #f5f6f7;
  padding-bottom: 100rpx;
}
.personal .personal-bg {
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: cover;
  padding-top: 90rpx;
}
.personal .personal-header {
  padding: 20rpx;
  font-size: 40rpx;
  padding-top: 60rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  color: #fff;
  position: fixed;
  width: 95%;
  z-index: 99999;
  top: 0;
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: 100%;
}
.personal .personal-header .position {
  font-size: 30rpx;
}
.personal .personal-header .position text:first-child {
  font-size: 33rpx;
  margin-right: 10rpx;
}
.personal .personal-header .position text:last-child {
  font-size: 18rpx;
  margin-left: 10rpx;
}
.personal .personal-header .title {
  margin-left: -80rpx;
}
.personal .personal-header > .iconfont {
  font-size: 48rpx;
}
.personal .personal-top {
  color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 30rpx 20rpx 100rpx;
  padding-top: 70rpx;
}
.personal .personal-top .personal-top-left {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.personal .personal-top .personal-top-left image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.personal .personal-top .personal-top-left .title {
  margin-top: -5rpx;
}
.personal .personal-top .personal-top-left .title .name {
  font-size: 38rpx;
}
.personal .personal-top .personal-top-left .title .dengji {
  margin-top: 15rpx;
