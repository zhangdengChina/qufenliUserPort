.search .search-top {
  height: 145rpx;
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: cover;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 0 30rpx;
  padding-top: 40rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.search .search-top > .iconfont {
  font-size: 40rpx;
  color: #fff;
}
.search .search-top .search-input {
  width: 75%;
  height: 70rpx;
  background: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-radius: 10rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-left: 30rpx;
}
.search .search-top .search-input .iconfont {
  font-size: 30rpx;
  color: #999;
  margin: 0 30rpx;
}
.search .search-top .search-input .input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  color: #000;
}
.search .search-top .btn {
  font-size: 28rpx;
  color: #fff;
}
.search .search-history {
  padding: 0 30rpx;
}
.search .search-history .item {
  color: #666;
  font-size: 28rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #f3f3f3;
}
.search .search-record {
  font-size: 28rpx;
  color: #666;
  padding-right: 30rpx;
  text-align: right;
  margin-top: 50rpx;
}
.search .search-record text {
  margin-right: 10rpx;
}

