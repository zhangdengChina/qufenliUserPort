.common-tabbar {
  color: #333;
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  background: #fff;
  position: fixed;
  bottom: 0;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  text-align: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.common-tabbar .active text {
  color: #f5671f;
}
.common-tabbar .item text {
  font-size: 48rpx;
}
.common-tabbar .item view {
  font-size: 20rpx;
}

.setting {
  position: relative;
  height: 100%;
  background: #f6f7f8;
  color: #333;
}
.setting .setting-header {
  padding: 20rpx;
  padding-top: 60rpx;
  font-size: 40rpx;
  background: -webkit-linear-gradient(333deg, #ff5a2b 0%, #ff5934 36%, #ff9c1f 100%);
  background: linear-gradient(117deg, #ff5a2b 0%, #ff5934 36%, #ff9c1f 100%);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  color: #fff;
  width: 95%;
}
.setting .setting-content {
  margin-top: 20rpx;
  background: #fff;
  padding-left: 30rpx;
}
.setting .setting-content .ispwd {
  display: none !important;
}
.setting .setting-content .setting-content-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-bottom: 1rpx solid #ccc;
  padding: 30rpx 30rpx 30rpx 0;
  font-size: 30rpx;
}
.setting .setting-content .setting-content-item .right input {
  width: 200rpx;
  font-size: 30rpx;
}
.setting .phoneMaskShow {
  display: none;
}
.setting .phoneMask {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background: rgba(0, 0, 0, 0.8);
  z-index: 9999999;
  font-size: 30rpx;
}
.setting .phoneMask .phoneMask-content {
  color: #333;
  position: absolute;
  top: -400rpx;
  bottom: 0;
  left: 0;
  right: 0;
  width: 80%;
  height: 355rpx;
  border-radius: 20rpx;
  background: #fff;
  margin: auto;
  padding-top: 30rpx;
}
.setting .phoneMask .phoneMask-content .phoneMask-content-title {
  padding-bottom: 20rpx;
  font-size: 37rpx;
  font-weight: bold;
  text-align: center;
  border-bottom: 1px solid #e0e0e0;
}
.setting .phoneMask .phoneMask-content .phoneMask-content-text {
  padding: 30rpx;
}
.setting .phoneMask .phoneMask-content .phoneMask-content-text text {
  color: #FF5A2C;
}
.setting .phoneMask .phoneMask-content .phoneMask-content-btn {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  padding-top: 30rpx;
}
.setting .phoneMask .phoneMask-content .phoneMask-content-btn text {
  width: 240rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 20rpx;
}
.setting .phoneMask .phoneMask-content .phoneMask-content-btn text:first-child {
  background: #F7F7F7;
}
.setting .phoneMask .phoneMask-content .phoneMask-content-btn text:last-child {
  background: #FF5A2C;
  color: #fff;
}
.hideBox {
  display: none;
}
.shopManage-testPwd {
  position: fixed;
  height: 100%;
  width: 100%;
  z-index: 9999;
  padding-top: 100rpx;
  top: 0;
  background: #fff;
}
.shopManage-testPwd .back {
  font-size: 30rpx;
  margin-left: 30rpx;
  margin-top: 30rpx;
  color: #FF5A2C;
}
.shopManage-testPwd .shopManage-testPwd-title {
  margin-top: 300rpx;
  text-align: center;
  font-size: 40rpx;
  font-weight: bold;
  margin-bottom: 100rpx;
}
.shopManage-testPwd .shopManage-testPwd-tips {
  text-align: center;
  margin-top: 20rpx;
  color: red;
  display: none;
}
.shopManage-testPwd .hideTips {
  display: block;
}
.submit-btn {
  width: 95%;
  background: -webkit-linear-gradient(207deg, #ff9960 0%, #ff5a2c 100%);
  background: linear-gradient(243deg, #ff9960 0%, #ff5a2c 100%);
  height: 88rpx;
  border-radius: 10rpx;
  color: #fff;
  font-size: 40rpx;
  margin: 50rpx auto 90rpx;
  text-align: center;
  line-height: 88rpx;
  box-shadow: 0 10rpx 20rpx #FF9960;
}

