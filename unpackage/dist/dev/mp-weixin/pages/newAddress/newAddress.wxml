<view class="newAddress"><common-header vue-id="1" headerTitl="{{title+'收货地址'}}" xingHide="true" lingHide="true" fenxiangHide="true" bind:__l="__l"></common-header><view class="newAddress-content"><view class="newAddress-content-item"><text>收货地址:</text><input type="text" placeholder-style="color:#999;fontSize:28rpx;textAlign:right" placeholder="请输入收货地址" data-event-opts="{{[['blur',[['getAddress',['$event']]]]]}}" value="{{address}}" bindblur="__e"/></view><view class="newAddress-content-item"><text>门牌口:</text><input class="twoInput" type="text" placeholder-style="color:#999;fontSize:28rpx;textAlign:right" placeholder="详细地址, 例: 16号楼5层501室" data-event-opts="{{[['blur',[['getRoomNum',['$event']]]]]}}" value="{{roomNum}}" bindblur="__e"/></view><view class="newAddress-content-item"><text>联系人:</text><input type="text" placeholder-style="color:#999;fontSize:28rpx;textAlign:right" placeholder="姓名/昵称" data-event-opts="{{[['blur',[['getUsername',['$event']]]]]}}" value="{{username}}" bindblur="__e"/></view><view class="newAddress-content-sex"><label class="radio"><radio value="先生" checked="{{checked}}" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e"></radio><text>先生</text></label><label class="radio"><radio value="女士" checked="{{!checked}}" data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" bindtap="__e"></radio><text>女士</text></label></view><view class="newAddress-content-item"><text>手机号:</text><input type="text" maxlength="11" placeholder-style="color:#999;fontSize:28rpx;textAlign:right" placeholder="请输入手机号" data-event-opts="{{[['blur',[['getPhone',['$event']]]]]}}" value="{{phone}}" bindblur="__e"/></view></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="submit-btn" bindtap="__e">{{''+title+''}}</view></view>